{
    "name": "yassin-ahmed/laravel-api-crud-generator",
    "description": "A comprehensive CRUD generator for Laravel API development",
    "keywords": ["laravel", "crud", "api", "generator", "artisan"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yassin",
            "email": "yassin.ahmed3070@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Yassin\\LaravelApiCrudGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yassin\\LaravelApiCrudGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yassin\\LaravelApiCrudGenerator\\CrudGeneratorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}